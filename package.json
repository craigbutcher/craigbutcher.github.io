{
  "name": "craigbutcher.io",
  "version": "1.0.0",
  "private": true,
  "description": "Personal Website",
  "author": "Craig Butcher",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "dryrun": "npx eleventy --dryrun",
    "start": "npx eleventy --serve",
    "watch": "npx eleventy --watch",
    "build": "npx eleventy && npm run copy:keybase",
    "debug": "DEBUG=* npx eleventy",
    "clean:css": "rm -rf /_site/assets/css/*",
    "copy:keybase": "cp src/keybase.txt _site/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigbutcher/personal-site.git"
  },
  "bugs": {
    "url": "https://github.com/craigbutcher/personal-site/issues"
  },
  "homepage": "https://github.com/craigbutcher/personal-site#readme",
  "devDependencies": {
    "@11ty/eleventy": "latest",
    "@11ty/eleventy-navigation": "latest",
    "@11ty/eleventy-plugin-inclusive-language": "latest",
    "@11ty/eleventy-plugin-rss": "latest",
    "@11ty/eleventy-plugin-syntaxhighlight": "latest",
    "luxon": "latest",
    "markdown-it": "latest",
    "markdown-it-anchor": "latest"
  }
}
